type Launch {
  id: ID! @id
  rocket_name: String
  rocket_type: String
  launch_date: String
  details: String
  flight_number: Int
  launch_success: Boolean
  article_link: String
  reuse: Reuse
  links: Links
  mission_patch_small: String
}

type Reuse {
  id: ID! @id
  core: Boolean
  side_core1: Boolean
  side_core2: Boolean
  fairings: Boolean
  capsule: Boolean
}

type Links {
  id: ID! @id
  reddit_campaign: String
  reddit_launch: String
  reddit_recovery: String
  reddit_media: String
}
