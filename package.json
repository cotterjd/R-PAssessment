{
  "name": "full-stack-assessment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all start-server seed",
    "stop": "docker-compose down",
    "start-prisma": "docker-compose -f docker-compose-dev.yml up -d",
    "stop-prisma": "docker-compose -f docker-compose-dev.yml down",
    "start-server": "docker-compose up -d --build",
    "seed": "(cd db && sleep 3 && node seed.js)",
    "push": "git push origin $(git branch | grep \\* | cut -d ' ' -f2)",
    "pull": "git pull origin $(git branch | grep \\* | cut -d ' ' -f2)"
  },
  "dependencies": {
    "babel-eslint": "7.2.3",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "graphql-client": "^2.0.1",
    "npm-run-all": "^4.1.2",
    "ramda": "^0.26.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
